pipeline {
  agent any
    
  tools {nodejs "nodejs"}
    
  stages {
    stage('Build') {
      steps {
        dir('backend'){
          sh 'npm install'
        }
      }
    }  
    stage('deploy to GCP') {
      steps {
        sshagent(['ssh-key']) {
            sh 'ssh -i mukherjeeabhilash8 mukherjeeabhilash8@35.192.185.129'
        }
      }
    }
  }
}