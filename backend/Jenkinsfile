pipeline {
  agent any
    
  tools {nodejs "nodejs"}
    
  stages {
    stage('Build') {
      steps {
        dir('backend'){
          sh 'npm install'
        }
      }
    }  
    stage('Deploy to GCP') {
      steps {
        sshagent(credentials: ['ssh-key']) {
            sh "ssh -o StrictHostKeyChecking=no -l mukherjeeabhilash8 35.192.185.129 pwd"
        }
      }
    }
  }
}